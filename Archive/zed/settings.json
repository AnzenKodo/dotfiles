{
  "ui_font_family": "0xProto",
  "tab_bar": {
    "show": false
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false,
    "selections_menu": false
  },
  "unstable.ui_density": "compact",
  "ensure_final_newline_on_save": true,
  "tab_size": 4,
  "wrap_guides": [80],
  "use_autoclose": false,
  "use_auto_surround": false,
  "soft_wrap": "editor_width",
  "preferred_line_length": 80,
  "hard_tabs": false,
  "show_whitespaces": "all",
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "autosave": "on_focus_change",
  "cursor_shape": "block",
  "indent_guides": {
    "coloring": "indent_aware"
  },
  "centered_layout": {
    "left_padding": 0,
    "right_padding": 0
  },
  "languages": {
    "HTML": {
      "use_auto_surround": false,
      "use_autoclose": false
    }
  },
  "lsp": {
    "vscode-html-language-server": {
      "settings": {
        "html": { "tagAutoclosing": false }
      }
    }
  },
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Gruvbox Dark Soft"
  },
  "format_on_save": "off",
  "chat_panel": {
    "button": false
  },
  "assistant": {
    "enabled": false,
    "version": "2"
  },
  "notification_panel": { "button": false },
  "collaboration_panel": { "button": false },
  "vim_mode": false,
  "features": {
    "inline_completion_provider": "none",
    "copilot": false
  },
  "auto_update": false,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  }
}
