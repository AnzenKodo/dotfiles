{
  // Font
  "ui_font_size": 16.5,
  "buffer_font_size": 16,
  "buffer_font_family": "0xProto",
  // Theme
  "theme": "One Dark",
  "icon_theme": "Catppuccin Frapp√©",
  // Look
  "notification_panel": {
    "dock": "left"
  },
  "agent": {
    "dock": "left",
    "version": "2"
  },
  "terminal": {
    "dock": "left"
  },
  "outline_panel": {
    "dock": "right"
  },
  "git_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "right"
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "gutter": {
    "folds": false,
    "code_actions": false,
    "runnables": false,
    "breakpoints": false,
    "line_numbers": false
  },
  "edit_predictions": {
    "mode": "subtle",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": true
  },
  "show_edit_predictions": false,
  "collaboration_panel": {
    "dock": "left"
  },
  "format_on_save": "off",
  "use_autoclose": false,
  "preferred_line_length": 80,
  "wrap_guides": [ 80 ],
  "show_wrap_guides": true,
  "autosave": "off",
  "unnecessary_code_fade": 0.6,
  "indent_guides.show": "always",
  "show_whitespaces": "selection",
  "remove_trailing_whitespace_on_save": true,
  "ensure_final_newline_on_save": true,
  "enable_preview_from_code_navigation": true,
  "current_line_highlight": "all",
  "base_keymap":"VSCode",
  "enable_language_server": true,
  "git": {
    "inline_blame": {
      "enabled": false
    }
  },
  "languages": {
    "C++": {
      "enable_language_server": false
    },
    "C": {
      "enable_language_server": false
    }
  },
  "buffer_font_features.calt": false,
  "soft_wrap": "editor_width",
  "diagnostics": {
    "include_warnings": false,
    "inline": {
      "enabled": false,
    }
  },
  // Useless
  "auto_update": false,
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
}
