cd $HOME/Dotfiles

git add .
git commit -m "Backup from Phone"
git push

rclone bisync $HOME/storage/shared/Drive Personal:\
    --config=$HOME/storage/shared/Drive/Dotfiles/rclone.conf \        
    --check-first --metadata --checksum --download-hash --verbose \
    --compare size,modtime,checksum \
    --conflict-resolve newer

